  month = jun,
  year = {2015},
  pages = {3357-68},
  file = {/home/jpfeil/Zotero/storage/8JBZCMTQ/Althoff et al. - 2015 - A Cre-conditional MYCN-driven neuroblastoma mouse model as an improved tool for preclinical studies.pdf},
  pmid = {25174395},
