  year = {2014},
  keywords = {antibody engineering,bispecific antibody,HER2,HER3,receptor internalization},
  pages = {340-53},
  file = {/home/jpfeil/Zotero/storage/5XSZP2JW/Kang et al. - 2014 - Engineering multivalent antibodies to target heregulin-induced HER3 signaling in breast cancer cells.pdf},
