  pages = {257-68},
  file = {/home/jpfeil/Zotero/storage/DU7W22G9/Villasante, Vunjak-Novakovic - 2015 - Tissue-engineered models of human tumors for cancer research.pdf},
  pmid = {25662589},
  publisher = {{NIH Public Access}}
}

