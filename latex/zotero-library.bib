  month = apr,
  year = {2019},
  keywords = {Genomic library construction,Genomic signal processing,Neurons,Polymerase chain reaction,Prefrontal cortex,Rett syndrome,Sequence alignment,Sequence motif analysis},
  pages = {e1008043},
  file = {/home/jpfeil/Zotero/storage/2GGXXYGX/Zhao et al. - 2019 - Somatic LINE-1 retrotransposition in cortical neur.pdf;/home/jpfeil/Zotero/storage/2UTG92H6/article.html}
