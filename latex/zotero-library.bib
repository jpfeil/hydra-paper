  month = oct,
  year = {2012},
  keywords = {Cancer,Gene expression profiling},
  pages = {1409-1417},
  file = {/home/jpfeil/Zotero/storage/3YLVLNJL/Schramm et al. - 2012 - Exon-level expression analyses identify MYCN and NTRK1 as major determinants of alternative exon usage and robus.pdf},
