  keywords = {cancer,bioinformatics tools,Mendelian disorders,next-generation sequencing,variants},
  pages = {256-78},
  file = {/home/jpfeil/Zotero/storage/D8AC55R4/Pabinger et al. - 2014 - A survey of tools for variant analysis of next-generation genome sequencing data.pdf},
