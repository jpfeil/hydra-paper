  month = jul,
  year = {2014},
  file = {/home/jpfeil/Zotero/storage/KMAK3WFT/Criscione et al. - 2014 - Transcriptional landscape of repetitive elements i.pdf},
  pmid = {25012247},
  pmcid = {PMC4122776}
}

