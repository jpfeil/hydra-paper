  month = jun,
  year = {2010},
  pages = {i237-i245},
  file = {/home/jpfeil/Zotero/storage/7SPZJ5LR/Vaske et al. - 2010 - Inference of patient-specific pathway activities from multi-dimensional cancer genomics data using PARADIGM.pdf},
  ids = {Vaske2010a},
