  month = aug,
  year = {2016},
  keywords = {cancer,neuroblastoma,cell migration,MYCN oncogene,TFAP4},
  pages = {54937-54951},
  file = {/home/jpfeil/Zotero/storage/YAIHKJD3/Xue et al. - 2016 - MYCN promotes neuroblastoma malignancy by establishing a regulatory circuit with transcription factor AP4.pdf},
  pmid = {27448979},
