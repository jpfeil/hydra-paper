   <text:p text:style-name="Text_20_body"><draw:frame draw:style-name="fr1" draw:name="Image60" text:anchor-type="as-char" svg:width="0.7862in" svg:height="0.3929in" draw:z-index="0"><draw:image xlink:href="main/TumorMap-NBL-MM-V3-2x.png" xlink:type="simple" xlink:show="embed" xlink:actuate="onLoad" loext:mime-type="image/png"/>
